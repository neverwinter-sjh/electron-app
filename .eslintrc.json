{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "extends": ["react"],
  "plugins": ["prettier"],
  // add your custom rules here
  "rules": {
    "no-console": "off",
    "no-unused-vars": "off",
    "vue/no-unused-components": "off",
    "require-await": "off"
  }
}
